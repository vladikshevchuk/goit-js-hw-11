{{#each this}}
    <div class="photo-card">
        {{!-- <img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" /> --}}
        <div class="gallery">
            <a href="{{largeImageURL}}"><img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" /></a>
        </div>
        <div class="info">
            <p class="info-item">
                <b>Likes <br>{{likes}}</b>
            </p>
            <p class="info-item">
                <b>Views <br>{{views}}</b>
            </p>
            <p class="info-item">
                <b>Comments <br>{{comments}}</b>
            </p>
            <p class="info-item">
                <b>Downloads <br>{{downloads}}</b>
            </p>
        </div>
    </div>
{{/each}}